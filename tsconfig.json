{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "outDir": ".",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "skipLibCheck": true
  },
  "files": [
    "./index.ts",
    "./test.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}